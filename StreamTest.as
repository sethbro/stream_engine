package  {    import StreamView;    public class StreamTest extends StreamView {        /**         *  Initialize video displays. Override for class variants.         */        override function initView() {            displays = {};            displays["cam_local"] = new StreamDisplay( 'cam_local', { show_info: false } );            displays["cam_local"].x = displays["cam_local"].y = 20;            addChild( displays["cam_local"] );            if ( options.showInfo ) {                initCamInfoPanel( displays["cam_local"].width + 30, 45 );            }            displays["server_relay"] = new StreamDisplay( 'server_relay', { show_info: options.showInfo } );            displays["server_relay"].x = 20;            displays["server_relay"].y = 310;            addChild( displays["server_relay"] );			        }        /**         *  Initialize connections. Override for class variants.         */        override function initConnections() {            ctrl = new StreamController( this );            ctrl.addConnection( 'cam_local', ctrl.input.cam, displays["cam_local"].vid );            ctrl.start( 'cam_local', { show_info : false } );            ctrl.addConnection( 'cam_server', ctrl.input.cam, serverURL );            ctrl.start( 'cam_server', options.streamName );            ctrl.addConnection( 'server_relay', serverURL, displays["server_relay"].vid );            ctrl.start( 'server_relay', options.streamName );        }    }}